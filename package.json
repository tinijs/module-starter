{
  "name": "@tinijs/module-starter",
  "version": "0.18.0-pre",
  "description": "The module starter.",
  "author": "Nhan Lam",
  "homepage": "https://tinijs.dev",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
